/*动画*/
.animated {
  -webkit-animation-duration: 1s; -moz-animation-duration: 1s; -ms-animation-duration: 1s; -o-animation-duration: 1s; animation-duration: 1s; 
  -webkit-animation-fill-mode: both; -moz-animation-fill-mode: both; -ms-animation-fill-mode: both; -o-animation-fill-mode: both; animation-fill-mode: both;
}

.animated.infinite {-webkit-animation-iteration-count: infinite; -moz-animation-iteration-count: infinite; -ms-animation-iteration-count: infinite; -o-animation-iteration-count: infinite; animation-iteration-count: infinite;}
.animated.linear{
  -webkit-animation-timing-function:linear;
  -moz-animation-timing-function:linear;
  -ms-animation-timing-function:linear;
  -o-animation-timing-function:linear;
  animation-timing-function:linear;
}
.animated.easeout{
  -webkit-animation-timing-function:ease-out;
  -moz-animation-timing-function:ease-out;
  -ms-animation-timing-function:ease-out;
  -o-animation-timing-function:ease-out;
  animation-timing-function:ease-out;
}
.animated.propertyAll{
    transition-property:all;
    -moz-transition-property:all; /* Firefox 4 */
    -webkit-transition-property:all; /* Safari 和 Chrome */
    -o-transition-property:all; /* Opera */
}
.animated.property-width{
  transition-property:width;
  -moz-transition-property:width; /* Firefox 4 */
  -webkit-transition-property:width; /* Safari 和 Chrome */
  -o-transition-property:width; /* Opera */
}
.animated.property-height{
  transition-property:height;
  -moz-transition-property:height; /* Firefox 4 */
  -webkit-transition-property:height; /* Safari 和 Chrome */
  -o-transition-property:height; /* Opera */
}
