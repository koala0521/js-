.animated.fast{-webkit-animation-duration: 300ms; -moz-animation-duration: 300ms; -ms-animation-duration: 300ms; -o-animation-duration: 300ms; animation-duration: 300ms;}
.animated.slow{-webkit-animation-duration: 2000ms; -moz-animation-duration: 2000ms; -ms-animation-duration: 2000ms; -o-animation-duration: 2000ms; animation-duration: 2000ms;}
