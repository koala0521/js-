/*ELF布局*/
/*elf-viewport,根容器 elf-viewport -> elf-viewport-body -> elf-view -> elf-view-body*/
.elf-fixed{width: 100%;height: 100%;}
.elf-viewport,
.elf-viewport-body,
.elf-view,
.elf-view-body,.elf-fixed-content-body{height:100%;width:100%;overflow:auto;position: relative;}
.elf-header,
.elf-footer{
	position:absolute;
	width:100%;
	height:60px;
}
.elf-header{left:0;top:0;z-index:2}
.elf-footer{left:0;bottom:0;z-index:2}
.elf-header ~ .elf-view{padding-top:60px;z-index:1}
.elf-footer ~ .elf-view{padding-bottom:60px;z-index:1}
/* 内容布局 */
.elf-content{width:100%;position:relative;}
.elf-content-body{width:100%;}

.elf-fixed-content{
	position:absolute;left:0;top:0;width:100%;height:100%;z-index: 1;
}
.elf-fixed .elf-content,
.elf-fixed .elf-content-body{height:100%;overflow: auto;}
/*elf-tbar elf-fbar medium large*/
.elf-tbar{position: absolute;top:0;left: 0;width:100%;z-index:10;height:32px;background:#fff;}
.elf-tbar.medium{height: 48px;}
.elf-tbar.large{height: 64px;}
.elf-tbar ~ .elf-content{padding-top:33px;z-index:1;}
.elf-tbar.medium ~ .elf-content{padding-top:49px;}
.elf-tbar.large ~ .elf-content{padding-top:65px;}
.elf-fbar{position: absolute;bottom:0;left:0;width:100%;z-index:10;height:32px;}
.elf-fbar.medium{height: 48px;}
.elf-fbar.large{height: 64px;}
.elf-fbar ~ .elf-content{padding-bottom:33px;z-index:1;}
.elf-fbar.medium ~ .elf-content{padding-bottom: 49px;}
.elf-fbar.large ~ .elf-content{padding-bottom: 65px;}
/*container 主要用于PC*/
.elf-container{margin-right: auto; margin-left: auto;width:1000px;}
.elf-container:before,.elf-container:after{display: table;content:"";}
.elf-container:after{clear: both;}
/* 栅格系统 */
.row:before,.row:after {display: table;content: " ";}.row:after {clear: both;}
