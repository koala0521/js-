.elf-accordion{background-color: #fff;display:block;}
.elf-accordion ul{display: block;list-style: inside none none;}
.elf-accordion li > ul{display: none;}
.elf-accordion li{position: relative;overflow: hidden;}
.elf-accordion li.opened > ul{
	display: block;
